# Ignore backup files
*~
\#*\#
/auto-save-list

# Byte-compiled Lisp files
**/*.elc
**/*.eln

**/*.eld
*.eld

# Elpaca, Straight, or other package manager build directories
/elpaca/
/straight/
/elpa/
/var/
/packages/

# Cache and session files
/.emacs.d/.cache
/.emacs.d/.recentf
/.emacs.d/places
/.emacs.d/url/
/.emacs.d/ido.last
/.emacs.d/history
/.emacs.d/bookmarks

# Tramp files
/.emacs.d/tramp

# Org-mode export files
**/*.html
**/*.pdf
**/*.tex

# Ignore projectile project files
/.projectile
/.projectile-cache

# Lockfiles (created when Emacs opens a file)
*.lock

# Directory local variables
.dir-locals.el
.local

# Generated files from Org mode (e.g., tangled init.el from config.org)
#init.el
config.el

# Personal customization files (if you're using custom-set-variables)
custom.el

